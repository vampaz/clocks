{
  "name": "Clocks",
  "version": 2,
  "builds": [
    {
      "src": "./**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "$1"
    },
    {
      "src": "/(assets/.+|.+\\.css|.+\\.js)",
      "headers": { "cache-control": "max-age=31536000,immutable" },
      "dest": "/$1"
    },
    {
      "src": "/(.*).html",
      "headers": { "cache-control": "public,max-age=0,must-revalidate" },
      "dest": "/$1.html"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "public,max-age=0,must-revalidate" },
      "dest": "/index.html"
    }
  ],
  "alias": [
    "conekto.eu"
  ]
}

